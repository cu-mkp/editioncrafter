{
  "name": "@cu-mkp/editioncrafter",
  "type": "module",
  "version": "1.1.0",
  "private": false,
  "description": "A simple digital critical edition publication tool",
  "license": "MIT",
  "homepage": "https://editioncrafter.org/",
  "repository": "https://github.com/cu-mkp/editioncrafter",
  "sideEffects": false,
  "main": "dist/editioncrafter.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "vite build",
    "build:dev": "vite serve",
    "build:es": "mkdir -p dist/es && cp -r src dist/es",
    "preview": "vite preview",
    "prepublishOnly": "npm run clean && npm run build && npm run build:es",
    "size": "bundlewatch --config bundlewatch.config.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@recogito/annotorious-openseadragon": "^2.7.11",
    "history": "^5.3.0",
    "html-react-parser": "^4.2.2",
    "openseadragon": "^4.1.0",
    "prop-types": "^15.5.10",
    "react-icons": "^4.8.0",
    "react-infinite-scroller": "^1.1.3",
    "react-markdown": "^8.0.7",
    "react-redux": "^8.0.5",
    "react-router": "^6.11.0",
    "react-router-dom": "^6.11.0",
    "react-scroll": "^1.7.10",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@chromatic-com/storybook": "^3.2.2",
    "@eslint-react/eslint-plugin": "^1.15.2",
    "@storybook/addon-essentials": "^8.4.3",
    "@storybook/addon-interactions": "^8.4.3",
    "@storybook/addon-links": "^8.4.3",
    "@storybook/blocks": "^8.4.3",
    "@storybook/builder-vite": "^8.4.3",
    "@storybook/react": "^8.4.3",
    "@storybook/react-vite": "^8.4.3",
    "@storybook/test": "^8.4.3",
    "@vitejs/plugin-react": "^4.3.3",
    "bundlewatch": "^0.3.3",
    "chalk": "^4.1.0",
    "codecov": "^3.7.0",
    "core-js": "^3.21.1",
    "css-loader": "^6.7.3",
    "enzyme": "^3.11.0",
    "eslint": "^9.14.0",
    "eslint-plugin-astro": "^1.3.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "glob": "^7.1.4",
    "http-server": "^14.1.0",
    "jsdom": "^19.0.0",
    "react-refresh": "^0.14.0",
    "redux-mock-store": "^1.5.1",
    "redux-saga-test-plan": "^4.0.6",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.61.0",
    "sass-loader": "^13.2.2",
    "storybook": "^8.4.2",
    "style-loader": "^3.3.2",
    "unfetch": "^4.1.0",
    "url-polyfill": "^1.1.7",
    "vite": "^5.4.11",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  },
  "overrides": {
    "@recogito/annotorious-openseadragon": {
      "openseadragon": "$openseadragon"
    }
  }
}
