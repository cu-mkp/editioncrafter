{
  "name": "@performant-software/editioncrafter",
  "version": "0.0.3",
  "description": "",
  "private": false,
  "main": "dist/cjs/src/index.js",
  "module": "dist/es/src/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./dist",
    "lint": "node_modules/.bin/eslint ./",
    "server": "node_modules/.bin/http-server --cors",
    "test": "npm run build && npm run lint && npm run size && jest -c jest.json",
    "test:debug": "node --inspect node_modules/.bin/jest -c jest.json --runInBand",
    "test:watch": "jest -c jest.json --watch",
    "build": "NODE_ENV=production && webpack --mode=production && npm run build:css",
    "build:dev": "webpack --mode=development",
    "build:es": "mkdir -p dist/es && cp -r src dist/es && NODE_ENV=production MODULE_FORMAT=es babel dist/es -d dist/es",
    "build:cjs": "mkdir -p dist/cjs && cp -r src dist/cjs && NODE_ENV=production MODULE_FORMAT=cjs babel dist/cjs -d dist/cjs",
    "build:css": "sass ./src/scss/:./dist && cp ./dist/editioncrafter.css ./stories/assets",
    "build:watch": "webpack --watch --mode=development",
    "prepublishOnly": "npm run clean && npm run build:es && npm run build:cjs && npm run build",
    "size": "bundlewatch --config bundlewatch.config.json",
    "start": "NODE_ENV=production webpack serve --open --port 5000",
    "watch-css": "sass --watch ./src/scss/:./__tests__/integration/editioncrafter/css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "license": "MIT",
  "repository": "https://github.com/cu-mkp/editioncrafter",
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "axios": "^1.3.4",
    "history": "^5.3.0",
    "html-react-parser": "^3.0.9",
    "openseadragon": "^4.0.0",
    "prop-types": "^15.5.10",
    "react-icons": "^4.8.0",
    "react-infinite-scroller": "^1.1.3",
    "react-redux": "^8.0.5",
    "react-router": "^6.11.0",
    "react-router-dom": "^6.11.0",
    "react-scroll": "^1.7.10",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.2"
  },
