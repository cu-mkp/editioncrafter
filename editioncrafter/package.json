{
  "name": "@cu-mkp/editioncrafter",
  "version": "1.0.3",
  "description": "A simple digital critical edition publication tool",
  "homepage": "https://cu-mkp.github.io/editioncrafter/",
  "private": false,
  "main": "dist/editioncrafter.min.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./dist",
    "lint": "node_modules/.bin/eslint ./",
    "build": "NODE_ENV=production && webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:es": "mkdir -p dist/es && cp -r src dist/es && NODE_ENV=production MODULE_FORMAT=es babel dist/es -d dist/es",
    "prepublishOnly": "npm run clean && npm run build:es && npm run build",
    "size": "bundlewatch --config bundlewatch.config.json",
    "start": "NODE_ENV=production webpack serve --open --port 5000",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "license": "MIT",
  "repository": "https://github.com/cu-mkp/editioncrafter",
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@recogito/annotorious-openseadragon": "^2.7.11",
    "history": "^5.3.0",
    "html-react-parser": "^4.2.2",
    "openseadragon": "^4.1.0",
    "prop-types": "^15.5.10",
    "react-icons": "^4.8.0",
    "react-infinite-scroller": "^1.1.3",
    "react-markdown": "^8.0.7",
    "react-redux": "^8.0.5",
    "react-router": "^6.11.0",
    "react-router-dom": "^6.11.0",
    "react-scroll": "^1.7.10",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-regenerator": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-interactions": "^7.4.1",
    "@storybook/addon-links": "^7.4.1",
    "@storybook/addon-styling-webpack": "0.0.4",
    "@storybook/blocks": "^7.4.1",
    "@storybook/react": "^7.4.1",
    "@storybook/react-webpack5": "^7.4.1",
    "@storybook/testing-library": "0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "bundlewatch": "^0.3.3",
    "chalk": "^4.1.0",
    "codecov": "^3.7.0",
    "core-js": "^3.21.1",
    "css-loader": "^6.7.3",
    "enzyme": "^3.11.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.4",
    "http-server": "^14.1.0",
    "jsdom": "^19.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-refresh": "^0.14.0",
    "redux-mock-store": "^1.5.1",
    "redux-saga-test-plan": "^4.0.6",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.61.0",
    "sass-loader": "^13.2.2",
    "storybook": "^7.4.1",
    "style-loader": "^3.3.2",
    "terser-webpack-plugin": "^5.3.1",
    "unfetch": "^4.1.0",
    "url-polyfill": "^1.1.7",
    "webpack": "^5.78.0",
    "webpack-cli": "^4.9.2"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "overrides": {
    "@recogito/annotorious-openseadragon": {
      "openseadragon": "$openseadragon"
    }
  }
}
